<% @hold = @collection.findAll({arc: "hold"}).toJSON() %>
<% @trial = @collection.findAll({arc: "trial"}).toJSON() %>
<% @assess = @collection.findAll({arc: "assess"}).toJSON() %>
<% @adopt = @collection.findAll({arc: "adopt"}).toJSON() %>
<section class="quadrant <%- @class %>">
  <%= @partial("radar-section", false, {name: "hold", collection: @hold}, url: @document.url) %>

  <%= @partial("radar-section", false, {name: "trial", collection: @trial}, url: @document.url) %>

  <%= @partial("radar-section", false, {name: "assess", collection: @assess}, url: @document.url) %>

  <%= @partial("radar-section", false, {name: "adopt", collection: @adopt}, url: @document.url) %>
</section>